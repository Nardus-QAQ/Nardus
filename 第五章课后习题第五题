/*第五章课后习题第五题*/
#include<stdio.h>
int main()
{
	float a,b;
	printf("Enter the income:");
	scanf("%f",&a);
	if(a<=750)
	b=0.01*a;
	else if(a<=2250)
	b=7.50+0.02*(a-750);
	else if(a<=3750)
	b=37.50+0.03*(a-2250);
	else if(a<=5250)
	b=82.50+0.04*(a-3750);
	else if(a<=7000)
	b=142.50+0.05*(a-5250);
	else if(a>7000)
	b=230.00+0.06*(a-7000);
	
	printf("The tax will be %f ",b);
	 
 } 
